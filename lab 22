import pandas as pd

df = pd.read_csv(r"C:\Users\Admin\Downloads\day22_interactions (1).csv")


df["mul_interaction"] = df["feature1"] * df["feature2"]

df["add_interaction"] = df["feature1"] + df["feature2"]

df["logical_interaction"] = (df["feature1"] > 0) & (df["feature2"] > 0)

print(df.corr()["target"])
